Rem PRINT FUNCTIONS
FUNCTION DRAW_FPS()
  INK RGB(255, 255, 0), RGB(0, 0, 0)
  TEXT 20, 20, "FPS: " + STR$(SCREEN FPS())
ENDFUNCTION

FUNCTION DRAW_VEC3(I)
  TEXT 20, 40, "X: " + STR$(X VECTOR3(I))
  TEXT 20, 60, "Y: " + STR$(Y VECTOR3(I))
  TEXT 20, 80, "Z: " + STR$(Z VECTOR3(I))
ENDFUNCTION
